  <xs:element name="Nutrition" type="xs:decimal"/>
  <xs:element name="techLevel" type="xs:NCName"/>
  <xs:element name="thingCategories">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="tickerType" type="xs:NCName"/>
  <xs:element name="tradeTags">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="storedConceptLearnOpportunity" type="xs:NCName"/>
  <xs:element name="thingSetMakerTags">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="tools">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ChemicalDef">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="defName"/>
        <xs:element ref="label"/>
        <xs:element ref="addictionHediff"/>
        <xs:element minOccurs="0" ref="canBinge"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="toleranceHediff"/>
          <xs:element ref="onGeneratedAddictedToleranceChance"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="onGeneratedAddictedEvents"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="addictionHediff" type="xs:NCName"/>
  <xs:element name="canBinge" type="xs:boolean"/>
  <xs:element name="toleranceHediff" type="xs:NCName"/>
