<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="Defs">
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="RecipeDef"/>
          <xs:element ref="ThingDef"/>
        </xs:choice>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="HediffDef"/>
          <xs:element ref="ChemicalDef"/>
          <xs:element ref="NeedDef"/>
          <xs:element ref="ThoughtDef"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="RecipeDef">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="defName"/>
        <xs:element ref="label"/>
        <xs:element ref="description"/>
        <xs:element ref="jobString"/>
        <xs:element ref="workSpeedStat"/>
        <xs:element ref="workSkill"/>
        <xs:element ref="effectWorking"/>
        <xs:element ref="soundWorking"/>
        <xs:element ref="allowMixingIngredients"/>
        <xs:element ref="workAmount"/>
        <xs:element ref="targetCountAdjustment"/>
        <xs:element ref="ingredients"/>
        <xs:element ref="fixedIngredientFilter"/>
        <xs:element ref="products"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="jobString" type="xs:string"/>
  <xs:element name="allowMixingIngredients" type="xs:boolean"/>
  <xs:element name="workAmount" type="xs:integer"/>
  <xs:element name="targetCountAdjustment" type="xs:integer"/>
  <xs:element name="ingredients">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="fixedIngredientFilter" type="thingDefs"/>
  <xs:element name="products">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Wort"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Wort" type="xs:integer"/>
  <xs:element name="ThingDef">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="thingClass"/>
          <xs:element ref="category"/>
          <xs:element ref="useHitPoints"/>
        </xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="defName"/>
          <xs:element ref="label"/>
          <xs:element ref="description"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="descriptionHyperlinks"/>
        <xs:element minOccurs="0" ref="tradeability"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="comps"/>
          <xs:element ref="altitudeLayer"/>
          <xs:element ref="alwaysHaulable"/>
          <xs:element ref="burnableByRecipe"/>
          <xs:element ref="costList"/>
          <xs:element ref="drawGUIOverlay"/>
          <xs:element ref="equipmentType"/>
          <xs:element ref="equippedAngleOffset"/>
          <xs:element ref="graphicData"/>
          <xs:element ref="healthAffectsPrice"/>
          <xs:element ref="ingestible"/>
          <xs:element ref="minRewardCount"/>
          <xs:element ref="pathCost"/>
          <xs:element ref="recipeMaker"/>
          <xs:element ref="resourceReadoutPriority"/>
          <xs:element ref="rotatable"/>
          <xs:element ref="selectable"/>
          <xs:element ref="socialPropernessMatters"/>
          <xs:element ref="stackLimit"/>
          <xs:element ref="statBases"/>
          <xs:element ref="techLevel"/>
          <xs:element ref="thingCategories"/>
          <xs:element ref="tickerType"/>
          <xs:element ref="tradeTags"/>
        </xs:choice>
        <xs:choice minOccurs="0">
          <xs:element ref="storedConceptLearnOpportunity"/>
          <xs:element ref="thingSetMakerTags"/>
          <xs:element ref="tools"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="Abstract" type="xs:NCName"/>
      <xs:attribute name="Name" type="xs:NCName"/>
      <xs:attribute name="ParentName" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="thingClass" type="xs:NCName"/>
  <xs:element name="category" type="xs:NCName"/>
  <xs:element name="useHitPoints" type="xs:boolean"/>
  <xs:element name="descriptionHyperlinks">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="HediffDef"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="tradeability" type="xs:NCName"/>
  <xs:element name="altitudeLayer" type="xs:NCName"/>
  <xs:element name="alwaysHaulable" type="xs:boolean"/>
  <xs:element name="burnableByRecipe" type="xs:boolean"/>
  <xs:element name="costList">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="Neutroamine"/>
        <xs:choice minOccurs="0">
          <xs:element ref="PsychoidLeaves"/>
          <xs:element ref="SmokeleafLeaves"/>
          <xs:element ref="Yayo"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Neutroamine" type="xs:integer"/>
  <xs:element name="PsychoidLeaves" type="xs:integer"/>
  <xs:element name="SmokeleafLeaves" type="xs:integer"/>
  <xs:element name="Yayo" type="xs:integer"/>
  <xs:element name="drawGUIOverlay" type="xs:boolean"/>
  <xs:element name="equipmentType" type="xs:NCName"/>
  <xs:element name="equippedAngleOffset" type="xs:integer"/>
  <xs:element name="graphicData">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="texPath"/>
        <xs:element ref="graphicClass"/>
        <xs:element minOccurs="0" ref="color"/>
        <xs:element minOccurs="0" ref="drawSize"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="texPath" type="xs:string"/>
  <xs:element name="graphicClass" type="xs:NCName"/>
  <xs:element name="color" type="xs:string"/>
  <xs:element name="drawSize" type="xs:decimal"/>
  <xs:element name="healthAffectsPrice" type="xs:boolean"/>
  <xs:element name="ingestible">
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="baseIngestTicks"/>
          <xs:element ref="chairSearchRadius"/>
          <xs:element ref="drugCategory"/>
          <xs:element ref="foodType"/>
          <xs:element ref="ingestCommandString"/>
          <xs:element ref="ingestEffect"/>
          <xs:element ref="ingestEffectEat"/>
          <xs:element ref="ingestHoldOffsetStanding"/>
          <xs:element ref="ingestHoldUsesTable"/>
          <xs:element ref="ingestReportString"/>
          <xs:element ref="ingestReportStringEat"/>
          <xs:element ref="ingestSound"/>
          <xs:element ref="joy"/>
          <xs:element ref="joyKind"/>
          <xs:element ref="maxNumToIngestAtOnce"/>
          <xs:element ref="nurseable"/>
          <xs:element ref="preferability"/>
          <xs:element ref="tasteThought"/>
          <xs:element ref="useEatingSpeedStat"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="canAutoSelectAsFoodForCaravan"/>
        <xs:element minOccurs="0" ref="outcomeDoers"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="baseIngestTicks" type="xs:integer"/>
  <xs:element name="chairSearchRadius" type="xs:integer"/>
  <xs:element name="drugCategory" type="xs:NCName"/>
  <xs:element name="foodType" type="xs:string"/>
  <xs:element name="ingestCommandString" type="xs:string"/>
  <xs:element name="ingestEffect" type="xs:NCName"/>
  <xs:element name="ingestEffectEat" type="xs:NCName"/>
  <xs:element name="ingestHoldOffsetStanding">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="northDefault"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="east"/>
          <xs:element ref="south"/>
          <xs:element ref="west"/>
        </xs:sequence>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="northDefault">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="offset"/>
        <xs:element minOccurs="0" ref="behind"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="behind" type="xs:boolean"/>
  <xs:element name="east">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="offset"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="south">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="offset"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="west">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="offset"/>
        <xs:element ref="flip"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="flip" type="xs:boolean"/>
  <xs:element name="ingestHoldUsesTable" type="xs:boolean"/>
  <xs:element name="ingestReportString" type="xs:string"/>
  <xs:element name="ingestReportStringEat" type="xs:string"/>
  <xs:element name="ingestSound" type="xs:NCName"/>
  <xs:element name="joy" type="xs:decimal"/>
  <xs:element name="joyKind" type="xs:NCName"/>
  <xs:element name="maxNumToIngestAtOnce" type="xs:integer"/>
  <xs:element name="nurseable" type="xs:boolean"/>
  <xs:element name="preferability" type="xs:NCName"/>
  <xs:element name="tasteThought">
    <xs:complexType/>
  </xs:element>
  <xs:element name="useEatingSpeedStat" type="xs:boolean"/>
  <xs:element name="canAutoSelectAsFoodForCaravan" type="xs:boolean"/>
  <xs:element name="outcomeDoers">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="minRewardCount" type="xs:integer"/>
  <xs:element name="pathCost" type="xs:integer"/>
  <xs:element name="recipeMaker">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="researchPrerequisite"/>
        <xs:element minOccurs="0" ref="recipeUsers"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="workSpeedStat"/>
          <xs:element ref="workSkill"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="effectWorking"/>
        <xs:element minOccurs="0" ref="soundWorking"/>
        <xs:choice minOccurs="0">
          <xs:element ref="bulkRecipeCount"/>
          <xs:sequence>
            <xs:element ref="skillRequirements"/>
            <xs:element ref="requiredGiverWorkType"/>
          </xs:sequence>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="researchPrerequisite" type="xs:NCName"/>
  <xs:element name="recipeUsers">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="bulkRecipeCount" type="xs:integer"/>
  <xs:element name="skillRequirements">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Cooking"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Cooking" type="xs:integer"/>
