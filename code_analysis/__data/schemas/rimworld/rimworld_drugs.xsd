          <xs:element ref="MarketValue"/>
          <xs:element ref="Mass"/>
          <xs:element ref="MaxHitPoints"/>
        </xs:choice>
        <xs:choice minOccurs="0">
          <xs:element ref="Beauty"/>
          <xs:element ref="Nutrition"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="WorkToMake" type="xs:integer"/>
  <xs:element name="DeteriorationRate" type="xs:decimal"/>
