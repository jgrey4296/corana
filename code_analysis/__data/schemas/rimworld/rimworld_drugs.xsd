  <xs:element name="stackLimit" type="xs:integer"/>
  <xs:element name="statBases">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="WorkToMake"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="DeteriorationRate"/>
