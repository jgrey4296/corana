    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="li"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="li">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="commonality"/>
        <xs:element ref="diseaseInc"/>
        <xs:element ref="max"/>
        <xs:element ref="maxFertility"/>
        <xs:element ref="min"/>
        <xs:element ref="minSize"/>
        <xs:element ref="perlinFrequency"/>
        <xs:element ref="terrain"/>
        <xs:element ref="thresholds"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="commonality" type="xs:integer"/>
  <xs:element name="diseaseInc" type="xs:NCName"/>
  <xs:element name="max" type="xs:decimal"/>
  <xs:element name="maxFertility" type="xs:decimal"/>
  <xs:element name="min" type="xs:decimal"/>
  <xs:element name="minSize" type="xs:integer"/>
  <xs:element name="perlinFrequency" type="xs:decimal"/>
  <xs:element name="terrain" type="xs:NCName"/>
