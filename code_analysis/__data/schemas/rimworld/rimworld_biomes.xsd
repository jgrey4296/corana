  <xs:element name="Caribou" type="xs:decimal"/>
  <xs:element name="Elk" type="xs:decimal"/>
  <xs:element name="Alpaca" type="xs:decimal"/>
  <xs:element name="Bear_Grizzly" type="xs:decimal"/>
  <xs:element name="Bear_Polar" type="xs:decimal"/>
  <xs:element name="Boomalope" type="xs:decimal"/>
  <xs:element name="Boomrat" type="xs:decimal"/>
  <xs:element name="Capybara" type="xs:decimal"/>
  <xs:element name="Cassowary" type="xs:decimal"/>
  <xs:element name="Chinchilla" type="xs:decimal"/>
  <xs:element name="Cougar" type="xs:decimal"/>
  <xs:element name="Deer" type="xs:decimal"/>
  <xs:element name="Dromedary" type="xs:decimal"/>
  <xs:element name="Elephant" type="xs:decimal"/>
