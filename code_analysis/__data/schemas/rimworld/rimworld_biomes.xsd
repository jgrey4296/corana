          <xs:element ref="Lynx"/>
          <xs:element ref="Megasloth"/>
          <xs:element ref="Muffalo"/>
          <xs:element ref="Ostrich"/>
          <xs:element ref="Raccoon"/>
          <xs:element ref="Rhinoceros"/>
          <xs:element ref="Tortoise"/>
          <xs:element ref="Turkey"/>
          <xs:element ref="Warg"/>
          <xs:element ref="WildBoar"/>
          <xs:element ref="Wolf_Arctic"/>
          <xs:element ref="Wolf_Timber"/>
        </xs:choice>
        <xs:sequence minOccurs="0">
          <xs:element ref="Cobra"/>
          <xs:element ref="Panther"/>
        </xs:sequence>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Donkey" type="xs:decimal"/>
  <xs:element name="GuineaPig" type="xs:decimal"/>
